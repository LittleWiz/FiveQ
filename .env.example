# MCP and LLM Configuration
# Copy this file to .env.local and add your actual API keys

# Anthropic Claude API Key (for direct LLM calls)
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# MCP Server Configuration (when you set up an MCP server)
VITE_MCP_SERVER_URL=http://localhost:3001
VITE_MCP_SERVER_ENABLED=false

# Question Generation Settings
VITE_QUESTIONS_PER_SET=10
VITE_QUESTION_CATEGORIES=Personal,Relationship,Dreams,Fun,Life
